reateObjectURL(blob);\n","      a.download = filename;\n","      div.appendChild(a);\n","      a.click();\n","      div.remove();\n","    }\n","  "]},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<IPython.core.display.Javascript object>"],"application/javascript":["download(\"download_bdb16d54-e9fe-4a5a-8433-cc5d3f04df33\", \"final_submission.csv\", 184576)"]},"metadata":{}}]}]}
